<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>LoginFilter</filter-name>
        <filter-class>com.xyzdrivers.LoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>LoginFilter</filter-name>
        <url-pattern>/AdminDashboard/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>AdminDashboard</servlet-name>
        <servlet-class>com.xyzdrivers.AdminDashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberDashboard</servlet-name>
        <servlet-class>com.xyzdrivers.MemberDashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MakeClaim</servlet-name>
        <servlet-class>com.xyzdrivers.MakeClaim</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.xyzdrivers.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.xyzdrivers.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberEdit</servlet-name>
        <servlet-class>com.xyzdrivers.MemberEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClaimEdit</servlet-name>
        <servlet-class>com.xyzdrivers.ClaimEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>com.xyzdrivers.Index</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminDashboard</servlet-name>
        <url-pattern>/AdminDashboard/View</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberDashboard</servlet-name>
        <url-pattern>/MemberDashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeClaim</servlet-name>
        <url-pattern>/MakeClaim</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberEdit</servlet-name>
        <url-pattern>/AdminDashboard/MemberEdit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClaimEdit</servlet-name>
        <url-pattern>/AdminDashboard/ClaimEdit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Index</welcome-file>
    </welcome-file-list>
</web-app>
